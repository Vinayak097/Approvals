{
  "name": "slack-approval-bot",
  "version": "1.0.0",
  "description": "A Slack bot for handling approval requests",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "vercel-build": "npm run build"
  },
  "keywords": [
    "slack",
    "bot",
    "approval",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@slack/events-api": "^3.0.1",
    "@slack/interactive-messages": "^2.0.2",
    "@slack/web-api": "^6.8.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "jest": "^29.4.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
}